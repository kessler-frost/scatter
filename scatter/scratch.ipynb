    "import redis\n",
    "\n",
    "r = redis.Redis()\n",
    "r.flushall()\n",
    "pipe = r.pipeline()\n",
    "\n",
    "pipe.hset(\"foo\", \"bar\", 0)\n",
    "pipe.execute()\n",
    "\n",
    "pipe.hset(\"foo\", \"haa\", 1)\n",
    "pipe.hset(\"foo\", \"zzz\", \"moo\")\n",
    "pipe.execute()"
   ]
